
UPDATE wp_options SET option_value = replace(option_value, 'http://nordglass.com.ua/', 'http://localhost4') WHERE option_name = 'home' OR option_name = 'siteurl';

UPDATE wp_posts SET guid = replace(guid, 'http://nordglass.com.ua/','http://localhost4');


UPDATE wp_posts SET post_content = replace(post_content, 'http://nordglass.com.ua/', 'http://localhost4');